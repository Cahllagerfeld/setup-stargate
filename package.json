{
  "name": "@cahllagerfeld/setup-stargate",
  "version": "0.3.2",
  "description": "Setup Local Stargate Container",
  "main": "dist/index.js",
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "start": "ts-node src/index.ts"
  },
  "bin": {
    "setup-stargate": "dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [
    "stargage"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@Cahllagerfeld"
  },
  "repository": {
    "url": "git://github.com/Cahllagerfeld/setup-stargate.git"
  },
  "author": "Cahllagerfeld",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/clear": "^0.1.1",
    "@types/commander": "^2.12.2",
    "@types/figlet": "^1.5.1",
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/inquirer": "^7.3.1",
    "axios": "^0.21.1",
    "chalk": "^4.1.1",
    "clear": "^0.1.0",
    "commander": "^7.2.0",
    "figlet": "^1.5.0",
    "inquirer": "^8.0.0"
  }
}
